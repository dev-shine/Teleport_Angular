{
  "name": "teleport-module-dev-portal",
  "version": "1.3.31",
  "description": "Dev Portal module for Shoutpoint's TelePort web app.",
  "main": "./bundle/module.umd.js",
  "types": "./bundle/index.d.ts",
  "scripts": {
    "fresh-start": "./node_modules/.bin/gulp 'fresh-project'",
    "bundle": "./node_modules/.bin/gulp 'bundle:prepare' && ./node_modules/.bin/ngc -p tsconfig.ngc.json && ./node_modules/webpack/bin/webpack.js -p --config webpack.config.js && ./node_modules/.bin/gulp 'bundle:finalize'",
    "bundle-with-sass": "./node_modules/.bin/gulp 'bundle:prepare' && ./node_modules/node-sass/bin/node-sass ./module -o ./module && ./node_modules/.bin/ngc -p tsconfig.ngc.json && ./node_modules/webpack/bin/webpack.js -p --config webpack.config.js && ./node_modules/.bin/gulp 'bundle:finalize'",
    "test": "npm run bundle && ./node_modules/karma/bin/karma start ./karma.conf.js --single-run",
    "demo-compile": "./node_modules/typescript/bin/tsc -p ./demo/tsconfig.json",
    "demo-run": "npm run demo-compile && ./node_modules/http-server/bin/http-server -o",
    "coverage-build": "./node_modules/.bin/remap-istanbul -i coverage/coverage-final.json -o coverage -t html",
    "coverage-open": "./node_modules/http-server/bin/http-server -c-1 -o -p 9875 ./coverage",
    "release-build": "npm run test && ./node_modules/.bin/gulp 'bump:patch'",
    "dist": "npm run bundle-with-sass",
    "tag": "npm version patch -m '@angular 4.3.6' && git push --tags"
  },
  "imports": [],
  "keywords": [
    "shoutpoint",
    "portal",
    "teleport"
  ],
  "author": "Shoutpoint, Inc.",
  "license": "SEE LICENSE IN LICENSE.md",
